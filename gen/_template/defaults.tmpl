{{- /*gotype: github.com/ogen-go/ogen/gen.TemplateConfig*/ -}}

{{ define "defaults" }}
{{ $pkg := $.Package }}
{{ template "header" $ }}

{{- range $_, $s := $.Types }}
{{- with $defaults := $s.DefaultFields }}

// setDefaults set default value of fields.
func (s *{{ $s.Name }}) setDefaults()  {
	{{- range $f := $defaults }}
	{
	{{- template "defaults/set" $f }}
	}
	{{- end }}
}
{{- end }}
{{- end }}

{{- end }}
