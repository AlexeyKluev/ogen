{{- /*gotype: github.com/ogen-go/ogen/internal/ir.Type*/ -}}
{{- define "schema/map" }}
// Map {{ $.Name }}
{{- if eq (len $.Fields) 0 }}{{/* No properties */}}
	type {{ $.Name }} map[string]{{ $.Item.Go }}
{{- else }}
	type {{ $.Name }} struct {
		Fields struct {
		{{- range $f := $.Fields }}
			{{ $f.Name }} {{ $f.Type.Go }} {{ if $f.Tag.JSON }}`json:"{{ $f.Tag.JSON }}"`{{ end }}
		{{- end }}
		}
		Addtional map[string]{{ $.Item.Go }}
	}
{{- end }}
{{ end }}
